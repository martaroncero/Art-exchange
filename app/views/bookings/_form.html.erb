<%= simple_form_for [@painting, @booking] do |f| %>
  <%= f.input :start_date, as: :string, input_html: { type: :date, value: Date.today } %>
  <%= f.input :end_date, as: :string, input_html: { type: :date, value: Date.today } %>
  <%= f.submit "Create booking", class: "btn btn-primary" %>
<% end %>